// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:data.db"
}

generator client {
  provider = "prisma-client-js"
}

model Problem {
    id Int @id @default(autoincrement())
    question String
    answer Boolean
    correct String
    incorrect String
}
